{
    "name": "detosphere/blog-package",
    "description": "A simple package to manage blog posts across projects in a conventional way.",
    "type": "library",
    "authors": [
        {
            "name": "Elikem Seake-Kwawu",
            "email": "skelikem@gmail.com"
        }
    ],
    "require": {
        "spatie/laravel-sluggable": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Detosphere\\BlogPackage\\": "src/",
            "Detosphere\\BlogPackage\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Detosphere\\BlogPackage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Detosphere\\BlogPackage\\BlogPackageServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
